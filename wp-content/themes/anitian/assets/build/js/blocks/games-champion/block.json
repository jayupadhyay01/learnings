{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "anitian/games-champion",
  "version": "0.1.0",
  "title": "Games Champion Block",
  "category": "games",
  "icon": "superhero",
  "description": "This block is for Champion section of Trader Games pages.",
  "keywords": [
    "Games",
    "Champion",
    "Block"
  ],
  "supports": {
    "html": false
  },
  "textdomain": "anitian",
  "attributes": {
    "textAlignment": {
      "type": "string",
      "default": "center"
    },
    "Heading": {
      "type": "string",
      "default": ""
    },
    "HeadingColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "headingVisiblity": {
      "type": "boolean",
      "default": true
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "descriptionColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "descriptionVisiblity": {
      "type": "boolean",
      "default": true
    },
    "buttonText": {
      "type": "string",
      "default": "Button"
    },
    "buttonTextColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "buttonBgColor": {
      "type": "string",
      "default": "#F9C744"
    },
    "buttonVisiblity": {
      "type": "boolean",
      "default": false
    },
    "BgGradientColor": {
      "type": "string",
      "default": "radial-gradient(circle, rgba(231,171,61,1) 0%, rgba(0,0,0,1) 0%, rgba(0,0,0,1) 100%, rgba(231,171,61,1) 100%)"
    },
    "subHeading": {
      "type": "string",
      "default": ""
    },
    "subHeadingColor": {
      "type": "string",
      "default": "#E1A142"
    },
    "subHeadingVisiblity": {
      "type": "boolean",
      "default": true
    },
    "Image": {
      "type": "object",
      "default": {
        "id": "",
        "url": "",
        "alt": "",
        "width": "",
        "height": "",
        "mediaMime": "",
        "mediaType": "",
        "cardID": "",
        "cardUrl": "",
        "cardAlt": "",
        "cardWidth": "",
        "cardHeight": ""
      }
    },
    "bgImage": {
      "type": "object",
      "default": {
        "id": "",
        "url": "",
        "alt": "",
        "width": "",
        "height": "",
        "mediaMime": "",
        "mediaType": ""
      }
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}