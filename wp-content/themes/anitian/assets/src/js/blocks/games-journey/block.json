    "buttonVisiblity": {
      "type": "boolean",
      "default": true
    },
    "button2Text": {
      "type": "string",
      "default": "Button"
    },
    "button2TextColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "button2BgColor": {
      "type": "string",
      "default": "#000000"
    },
    "button2Visiblity": {
      "type": "boolean",
      "default": true
    },
    "BgGradientColor": {
      "type": "string",
      "default": "radial-gradient(circle, rgba(231,171,61,1) 0%, rgba(0,0,0,1) 0%, rgba(0,0,0,1) 100%, rgba(231,171,61,1) 100%)"
    },
    "subHeading": {
      "type": "string",
      "default": ""
    },
    "subHeadingColor": {
      "type": "string",
      "default": "#E1A142"
    },
    "subHeadingVisiblity": {
      "type": "boolean",
      "default": false
    },
    "bgImage": {
      "type": "object",
      "default": {
          "id": "",
          "url": "",
          "alt": "",
          "width": "",
          "height": "",
          "mediaMime": "",
          "mediaType": ""
      }
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
