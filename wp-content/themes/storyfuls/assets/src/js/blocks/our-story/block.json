{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "storyful/our-story",
  "version": "0.1.0",
  "title": "Our Story Block",
  "category": "storyful",
  "icon": "media-text",
  "description": "This Block is for Our Story section of the storyful pages.",
  "keywords": ["Our", "Story", "Block"],
  "supports": {
    "html": false
  },
"textdomain": "storyful",
   "attributes": {
    "bgColor": {
      "type": "string",
      "default": ""
    },
    "textAlignment": {
        "type": "string",
        "default": "left"
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "descriptionColor": {
      "type": "string",
      "default": ""
    },
    "showBgOverlay": {
			"type": "boolean",
			"default": false
		},
    "bgImage": {
      "type": "object",
      "default": {
          "id": "",
          "url": "",
          "alt": "",
          "width": "",
          "height": "",
          "mediaMime": "",
          "mediaType": ""
      }
    },
    "Image": {
      "type": "object",
      "default": {
          "id": "",
          "url": "",
          "alt": "",
          "width": "",
          "height": "",
          "mediaMime": "",
          "mediaType": ""
      }
    },
    "Video": {
      "type": "object",
      "default": {
          "id": "",
          "url": "",
          "alt": "",
          "width": "",
          "height": "",
          "mediaMime": "",
          "mediaType": ""
      }
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}