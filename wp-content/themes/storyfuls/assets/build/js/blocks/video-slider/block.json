{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "storyful/video-slider",
  "version": "0.1.0",
  "title": "Video Slider Block",
  "category": "storyful",
  "icon": "video-alt3",
  "description": "This block is for Video Slider section of storyful pages.",
  "keywords": [
    "Video",
    "Slider",
    "Block"
  ],
  "textdomain": "storyful",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "titleVisiblity": {
      "type": "boolean",
      "default": false
    },
    "bgColor": {
      "type": "string",
      "default": ""
    },
    "textAlignment": {
      "type": "string",
      "default": "center"
    },
    "ArrayData": {
      "type": "array",
      "default": []
    },
    "slideAuto": {
      "type": "boolean",
      "default": false
    },
    "showDots": {
      "type": "boolean",
      "default": true
    },
    "showArrows": {
      "type": "boolean",
      "default": false
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}