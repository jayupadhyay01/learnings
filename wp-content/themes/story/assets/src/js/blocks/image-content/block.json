{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "storyful/image-content",
  "version": "0.1.0",
  "title": "Image Content Block",
  "category": "storyful",
  "icon": "cover-image",
  "description": "This block is for Image Content section of storyful pages.",
  "keywords": ["Image", "Content", "Block"],
  "supports": {
    "html": false
  },
"textdomain": "storyful",
   "attributes": {
    "bgColor": {
      "type": "string",
      "default": ""
    },
    "textAlignment": {
        "type": "string",
        "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "titleVisiblity": {
        "type": "boolean",
        "default": true
    },
    "descriptionColor": {
      "type": "string",
      "default": ""
    },
    "descriptionVisiblity": {
        "type": "boolean",
        "default": true
    },
    "listItems": {
      "type": "array",
      "default": []
    },
    "Image": {
        "type": "object",
        "default": {
            "id": "",
            "url": "",
            "alt": "",
            "width": "",
            "height": ""
        }
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}