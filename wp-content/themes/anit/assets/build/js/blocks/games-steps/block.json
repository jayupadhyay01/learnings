{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "anitian/games-steps",
  "version": "0.1.0",
  "title": "Games Steps Block",
  "category": "games",
  "icon": "editor-ol",
  "description": "This block is for steps section of Trader Games pages.",
  "keywords": [
    "Games",
    "Steps",
    "Block"
  ],
  "supports": {
    "html": false
  },
  "textdomain": "anitian",
  "attributes": {
    "textAlignment": {
      "type": "string",
      "default": "center"
    },
    "Heading": {
      "type": "string",
      "default": ""
    },
    "HeadingColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "headingVisiblity": {
      "type": "boolean",
      "default": true
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "descriptionColor": {
      "type": "string",
      "default": "#888888"
    },
    "descriptionVisiblity": {
      "type": "boolean",
      "default": true
    },
    "buttonText": {
      "type": "string",
      "default": "Button"
    },
    "buttonTextColor": {
      "type": "string",
      "default": "#FFF"
    },
    "buttonBgColor": {
      "type": "string",
      "default": "#000000"
    },
    "cardBgColor": {
      "type": "string",
      "default": "linear-gradient(70deg, rgba(226,167,72,1) 25%, rgba(252,232,103,1) 50%, rgba(226,167,72,1) 75%)"
    },
    "cardActiveBgColor": {
      "type": "string",
      "default": "linear-gradient(106deg, rgb(252, 228, 99) 31%, rgb(248, 187, 55) 85%)"
    },
    "BgGradientColor": {
      "type": "string",
      "default": "radial-gradient(circle, rgba(231,171,61,1) 0%, rgba(0,0,0,1) 0%, rgba(0,0,0,1) 100%, rgba(231,171,61,1) 100%)"
    },
    "subHeading": {
      "type": "string",
      "default": "How To Enter"
    },
    "subHeadingColor": {
      "type": "string",
      "default": "#E1A142"
    },
    "subHeadingVisiblity": {
      "type": "boolean",
      "default": true
    },
    "listItems": {
      "type": "array",
      "default": []
    },
    "cardSubTitleColor": {
      "type": "string",
      "default": "#00000"
    },
    "cardSubTitleVisiblity": {
      "type": "boolean",
      "default": true
    },
    "cardTitleColor": {
      "type": "string",
      "default": "#00000"
    },
    "cardTitleVisiblity": {
      "type": "boolean",
      "default": true
    },
    "cardDescriptionColor": {
      "type": "string",
      "default": "#00000"
    },
    "cardDescriptionVisiblity": {
      "type": "boolean",
      "default": true
    },
    "cardButtonVisiblity": {
      "type": "boolean",
      "default": true
    },
    "bgImage": {
      "type": "object",
      "default": {
        "id": "",
        "url": "",
        "alt": "",
        "width": "",
        "height": "",
        "mediaMime": "",
        "mediaType": ""
      }
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}