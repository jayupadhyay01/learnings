{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "anitian/games-banner",
  "version": "0.1.0",
  "title": "Games Banner Block",
  "category": "games",
  "icon": "button",
  "description": "This block is for Banner section of Trader Games pages.",
  "keywords": [
    "Games",
    "Banner",
    "Block"
  ],
  "supports": {
    "html": false
  },
  "textdomain": "anitian",
  "attributes": {
    "textAlignment": {
      "type": "string",
      "default": "center"
    },
    "Heading": {
      "type": "string",
      "default": ""
    },
    "HeadingColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "headingVisiblity": {
      "type": "boolean",
      "default": true
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "descriptionColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "descriptionVisiblity": {
      "type": "boolean",
      "default": false
    },
    "buttonText": {
      "type": "string",
      "default": "I Want to Know How"
    },
    "buttonTextColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "buttonBgColor": {
      "type": "string",
      "default": "linear-gradient(85deg, rgba(231,171,61,1) 0%, rgba(0,0,0,1) 19%, rgba(0,0,0,1) 79%, rgba(231,171,61,1) 100%)"
    },
    "buttonVisiblity": {
      "type": "boolean",
      "default": true
    },
    "BgGradientColor": {
      "type": "string",
      "default": "radial-gradient(rgb(231,171,61) 0%,rgb(0,0,0) 1%,rgb(0,0,0) 79%,rgb(231,171,61) 100%)"
    },
    "bgImage": {
      "type": "object",
      "default": {
        "id": "",
        "url": "",
        "alt": "",
        "width": "",
        "height": "",
        "mediaMime": "",
        "mediaType": ""
      }
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}