{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "anitian/media-content",
  "version": "0.1.0",
  "title": "Media Content Block",
  "category": "anitian",
  "icon": "admin-media",
  "description": "This block is for Media Content section of anitian pages.",
  "keywords": ["media","content", "Block"],
  "supports": {
    "html": false
  },
  "textdomain": "anitian",
  "attributes": {
    "textAlignment": {
      "type": "string",
      "default": "Left"
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "subTitleVisibility": {
      "type": "boolean",
      "default": true
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "subtitleColor": {
      "type": "string",
      "default": ""
    },
    "descriptionColor": {
      "type": "string",
      "default": ""
    },
    "titleVisiblity": {
      "type": "boolean",
      "default": true
    },
    "descriptionVisiblity": {
      "type": "boolean",
      "default": true
    },
    "bgColor": {
      "type": "string",
      "default": ""
    },
    "Image": {
      "type": "object",
      "default": {
          "id": "",
          "url": "",
          "alt": "",
          "width": "",
          "height": "",
          "mediaMime": "",
          "mediaType": ""
      }
    },
    "layout": {
      "type": "string",
      "default": "layout1"
    },
    "listItems": {
      "type": "array",
      "default": []
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}